# Discord Bot Token
# Obtén tu token en: https://discord.com/developers/applications
DISCORD_TOKEN=your_discord_bot_token_here

# MongoDB Database
# Conexión a MongoDB (local o remota)
MONGODB_URI=mongodb://localhost:27017
MONGODB_DB_NAME=discord_music_bot

# Spotify API Credentials (Opcional)
# Obtén tus credenciales en: https://developer.spotify.com/dashboard
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here

# Idioma por defecto (es, en, ar, pt)
DEFAULT_LANGUAGE=es
